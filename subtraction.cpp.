#include <iostream>
#include <windows.h>
double subtract(double a, double b)
{
    return a - b;
}
int main()
{
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    double num1, num2;
    std::cout << "Введіть перше число: ";
    std::cin >> num1;
    std::cout << "Введіть друге число: ";
    std::cin >> num2;
    double result = subtract(num1, num2);
    std::cout << "Результат віднімання: " << result << "\n";
    return 0;
}
